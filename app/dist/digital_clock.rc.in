1	ICON	"${WIN_ICO_FILE}"
1	VERSIONINFO
FILEVERSION	${PROJECT_VERSION_MAJOR},${PROJECT_VERSION_MINOR},${PROJECT_VERSION_PATCH},${BUILD_NUMBER}
PRODUCTVERSION	${PROJECT_VERSION_MAJOR},${PROJECT_VERSION_MINOR},${PROJECT_VERSION_PATCH},0
FILEFLAGSMASK	0x3F
FILEFLAGS	0x0
FILEOS		0x40004
FILETYPE	0x1
FILESUBTYPE	0x0
{
	BLOCK "StringFileInfo"
	{
		BLOCK "040904E4"
		{
			VALUE "Comments", "${PROJECT_DESCRIPTION}"
			VALUE "CompanyName", "Nick Korotysh"
			VALUE "FileDescription", "Digital Clock 5"
			VALUE "FileVersion", "${PROJECT_VERSION_MAJOR}.${PROJECT_VERSION_MINOR}.${PROJECT_VERSION_PATCH}.${BUILD_NUMBER}"
			VALUE "InternalName", "${PROJECT_NAME}.exe"
			VALUE "LegalCopyright", "(c) 2013-2025 Nick Korotysh"
			VALUE "OriginalFilename", "${PROJECT_NAME}.exe"
			VALUE "ProductName", "Digital Clock"
			VALUE "ProductVersion", "${PROJECT_VERSION_MAJOR}.${PROJECT_VERSION_MINOR}.${PROJECT_VERSION_PATCH}"
		}
	}

	BLOCK "VarFileInfo"
	{
		VALUE "Translation", 0x0409, 0x04E4
	}
}
